{
    "name": "omaralalwi/laravel-deployer",
    "description": "Laravel Deployer: Streamlined Deployment for Laravel and Node.js, with Zero-Downtime and various environments and branches, deploying your Laravel apps become simple and stress-free endeavor.",
    "keywords": [
        "omaralalwi",
        "laravel-deployer",
        "laravel",
        "deployer",
        "deployment",
        "continuous integration",
        "automation",
        "deployment tool",
        "php",
        "laravel package",
        "open source"
    ],
    "homepage": "https://github.com/omaralalwi/laravel-deployer",
    "license": "MIT",
    "type": "library",
    "version": "1.0.0",
    "authors": [
        {
            "name": "omar alalwi",
            "email": "omaralwi2010@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1|^8.2|^8.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5.3|^9.3.3|^10.5"
    },
    "autoload": {
        "psr-4": {
            "Omaralalwi\\LaravelDeployer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Omaralalwi\\LaravelDeployer\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Omaralalwi\\LaravelDeployer\\LaravelDeployerServiceProvider"
            ]
        }
    },
    "support": {
        "issues": "https://github.com/omaralalwi/laravel-deployer/issues",
        "source": "https://github.com/omaralalwi/laravel-deployer"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
